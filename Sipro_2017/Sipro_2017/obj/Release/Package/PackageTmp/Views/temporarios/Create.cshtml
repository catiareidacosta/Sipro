@model Sipro_2017.temporario

@{
    ViewBag.Title = "Create";
}

<h2>Novo</h2>

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    <div class="center-div">
        <div>
            <input type="submit" value="Gravar" />
            <input type="button" value="Cancelar" onclick="forwardMe('index')">
            <script> function forwardMe(url) { window.location.href = url;} </script>
        </div>
    </div>
    <div class="form-horizontal">
        <h4>temporario</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(model => model.dataAdmissao, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.TextBoxFor(model => model.dataAdmissao, "{0:dd-MM-yyyy}", new { @class = "datepicker", @size = "8" })
                @Html.ValidationMessageFor(model => model.dataAdmissao, "", new { @class = "text-danger" })
            </div>
        </div>

    </div>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
